# Configuration file for dormant accounts (USER and SERVICES)

# Number of days without login before user account is considered dormant
DORMANT_USERACCOUNT_DURATION=70  # Example: 70 days

# Number of days without login before service account is considered dormant
DORMANT_SERVICEACCOUNT_DURATION=30  # Example: 30 days

# Number of days before password must be changed (password expiry duration)
DORMANT_PASSWORD_EXPIRY_DURATION=90  # Example: 60 days
